<?xml version="1.0" encoding="UTF-8"?>
<configuration>
<appSettings>
    <!-- Required settings -->
    <add key="WSGI_HANDLER" value="django.core.wsgi.get_wsgi_application()" />
    <!-- Your django path -->
    <!-- <add key="PYTHONPATH" value="C:\inetpub\wwwroot\spotlight-main" />  -->
    <!-- Your djangoname.settings -->
    <add key="DJANGO_SETTINGS_MODULE" value="spotlightapi.settings" />
</appSettings>
    <system.webServer>
        <handlers>
            <remove name="spotlight" />
            <!-- <add name="spotlight" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Python311\python.exe|C:\Python311\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" /> -->
        </handlers>
        <httpProtocol>
            <customHeaders>
                <add name="Access-Control-Allow-Origin" value="*" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
